        id: container
        size_hint_y: None
        size_hint_x: 1
        height: self.minimum_height
        orientation: 'lr-tb'
        padding:'15dp', '15dp','15dp','15dp'
        spacing:'15dp'
        # UsuarioWidget:
        # UsuarioWidget:

<UsuarioWidget@StackLayout>:
