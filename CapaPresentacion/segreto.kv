                    background_down:'data/images/src/but_new_down.png'
                    on_release: root.open_TipoServicioPopup()

            StackLayout:
                size_hint_y: None
                height: '40dp'
                LabelGeneric:
                    text: 'Cantidad Guias:'
                    size_hint_x: .20
                CampoGenericoMediumR:
                    id: txt_number_labels
                    size_hint_x: .80
                    input_type: 'number'
                    input_filter: 'int'
                    text: "1"

            # Fila 2
            StackLayout:
                size_hint_y: None
                height: '40dp'
                LabelGeneric:
                    text: 'Entregar en Estafeta:'
                    size_hint_x: .20
                CheckBox:
                    id: chk_deliverytoestafetaoffice
                    size_hint_x: .60
