        BoxLayout:
            orientation:'horizontal'
            spacing:'16dp'
            size_hint_x: .1
            Space:
            # SegButton:
            #     id:but_del
            #     size_hint:None, None
            #     size:'36dp', '36dp'
            #     border:8,8,8,8
            #     background_normal:'data/images/src/but_del.png'
            #     background_down:'data/images/src/but_del_down.png'
                # on_release:root.dispatch('on_del_pressed')
            SegButton:
                id:but_edit
